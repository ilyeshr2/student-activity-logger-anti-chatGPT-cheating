{
    "manifest_version": 3,
    "name": "Student Activity Logger",
    "version": "1.0",
    "description": "Logs student activity and sends it to the server.",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
        "webNavigation",
        "webRequest",
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
      "https://chatgpt.com/"
    ]
  }
  